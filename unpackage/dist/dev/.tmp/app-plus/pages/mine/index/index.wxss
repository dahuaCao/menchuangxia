page {
  background: #F5F5F5;
  font-size: 28rpx;
}
.mb-wrap {
  background: #fff;
}
.personal-info {
  height: 196rpx;
  padding: 24rpx 28rpx 0;
}
.personal-info .info-lf .info-img {
    height: 138rpx;
    width: 138rpx;
    margin-right: 30rpx;
}
.personal-info .info-lf .info-img .icon {
      font-size: 144rpx;
      color: #2574EB;
}
.personal-info .info-lf .info-c {
    padding-top: 10rpx;
    line-height: 1.8;
    width: 0;
}
.personal-info .info-rf {
    width: 100rpx;
    -webkit-box-pack: end;
    -webkit-justify-content: flex-end;
        -ms-flex-pack: end;
            justify-content: flex-end;
    padding-top: 30rpx;
}
.personal-info .info-rf .icon {
      font-size: 76rpx;
      color: #51595D;
}
.list-content {
  padding: 0 30rpx 170rpx;
}
.list-content .list-cell {
    height: 100rpx;
    border-bottom: 1px solid #CDCDCD;
}
.list-content .list-cell .list-lf {
      font-size: 32rpx;
}
.list-content .list-cell .list-lf .icon {
        font-size: 48rpx;
        margin-right: 20rpx;
}
.list-content .list-cell .list-lf .yhj {
        color: #D9220A;
}
.list-content .list-cell .list-lf .ad {
        color: #12B2A6;
}
.list-content .list-cell .list-lf .hc {
        color: #FF3030;
}
.list-content .list-cell .list-lf .yj-fg {
        color: #509FD8;
}
.list-content .list-cell .list-lf .profit {
        color: #F96400;
}
.list-content .list-cell .list-lf .c-manger {
        color: #16C7E4;
        font-size: 60rpx;
        position: relative;
        left: -4rpx;
        margin-right: 12rpx;
}
.list-content .list-cell .list-lf .m-set {
        color: #2574EB;
}
.list-content .list-cell .lsit-rf .arrow {
      font-size: 48rpx;
      color: #333;
}


  font-weight: bolder;
}
.wxParse .p {
  margin: 1em 0;
}
.wxParse .i,
.wxParse .cite,
.wxParse .em,
.wxParse .var,
.wxParse .address {
  font-style: italic;
}
.wxParse .pre,
.wxParse .tt,
.wxParse .code,
.wxParse .kbd,
.wxParse .samp {
  font-family: monospace;
}
.wxParse .pre {
  overflow: auto;
  background: #f5f5f5;
  padding: 16rpx;
  white-space: pre;
  margin: 1em 0rpx;
}
.wxParse .code {
  display: inline;
  background: #f5f5f5;
}
.wxParse .big {
  font-size: 1.17em;
}
.wxParse .small,
.wxParse .sub,
.wxParse .sup {
  font-size: 0.83em;
}
.wxParse .sub {
  vertical-align: sub;
}
.wxParse .sup {
  vertical-align: super;
}
.wxParse .s,
.wxParse .strike,
.wxParse .del {
  text-decoration: line-through;
}
.wxParse .strong,
.wxParse .s {
